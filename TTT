local isEnabled = false
local isReachEnabled = false
local isKillEnabled = false
local notificationShown = false
local player = game.Players.LocalPlayer
local waitAfterDeath = false

local function isForbiddenPart(part)
for _, d in pairs(part:GetDescendants()) do
if (d:IsA("TextLabel") or d:IsA("TextButton")) and string.find(d.Text, "ÿßŸÑŸÖÿßÿ®") then
return true
end
end
return false
end

local function isForbiddenNearby(targetPart)
local forbiddenDistance = 5
for _, part in pairs(workspace:GetDescendants()) do
if part:IsA("BasePart") and isForbiddenPart(part) then
if (part.Position - targetPart.Position).Magnitude <= forbiddenDistance then
return true
end
end
end
return false
end

local function isInsideAnyBackpack(part)
for _, plr in pairs(game.Players:GetPlayers()) do
if plr:FindFirstChild("Backpack") and part:IsDescendantOf(plr.Backpack) then
return true
end
if plr.Character and part:IsDescendantOf(plr.Character) then
return true
end
end
return false
end

local function autoFarm()
local allParts = {}
for _, part in pairs(workspace:GetDescendants()) do
if (part:IsA("PointLight") or part:IsA("SpotLight") or part:IsA("ParticleEmitter")) and not isInsideAnyBackpack(part) then
table.insert(allParts, part.Parent)
end
end
for _, part in pairs(allParts) do
if not isForbiddenPart(part) and not isForbiddenNearby(part) then
if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
player.Character.HumanoidRootPart.CFrame = part.CFrame + Vector3.new(0, 3, 0)
return
end
end
end
end

local function startAutoFarm()
while isEnabled do
if waitAfterDeath then
task.wait(3)
waitAfterDeath = false
end
autoFarm()
task.wait(0.2)
end
end

local function autoKill()
while isKillEnabled do
if player.Character and player.Character:FindFirstChild("Humanoid") and player.Character.Humanoid.Health > 0 then
player.Character.Humanoid.Health = 0
end
task.wait(0.2)
end
end

player.CharacterAdded:Connect(function()
waitAfterDeath = true
if isKillEnabled then
task.spawn(autoKill)
end
end)

toggleBox.MouseButton1Click:Connect(function()
isEnabled = not isEnabled
checkMark.Visible = isEnabled
toggleLabel.Text = isEnabled and "ÿ™ÿπÿ∑ŸäŸÑ ÿ£Ÿàÿ™Ÿà ŸÅÿßÿ±ŸÖ" or "ÿ™ŸÅÿπŸäŸÑ ÿ£Ÿàÿ™Ÿà ŸÅÿßÿ±ŸÖ"

if isEnabled then  
    task.spawn(startAutoFarm)  
end

end)

reachToggleBox.MouseButton1Click:Connect(function()
isReachEnabled = not isReachEnabled
reachCheckMark.Visible = isReachEnabled
reachToggleLabel.Text = isReachEnabled and "ÿ™ÿπÿ∑ŸäŸÑ ÿ±Ÿäÿ™ÿ¥" or "ÿ™ŸÅÿπŸäŸÑ ÿ±Ÿäÿ™ÿ¥"

if isReachEnabled then  
    loadstring(game:HttpGet("https://pastebin.com/raw/SP5WGAJs"))()  
end

end)

killToggleBox.MouseButton1Click:Connect(function()
isKillEnabled = not isKillEnabled
killCheckMark.Visible = isKillEnabled

if isKillEnabled then  
    task.spawn(autoKill)  
end

end)

teleportToggleBox.MouseButton1Click:Connect(function()
if teleportCheckMark.Visible == false then
teleportCheckMark.Visible = true
loadstring(game:HttpGet("https://pastebin.com/raw/cH0LWX3q"))()
else
teleportCheckMark.Visible = false
end
end)

closeButton.MouseButton1Click:Connect(function()
mainFrame.Visible = false
end)

openButton.MouseButton1Click:Connect(function()
mainFrame.Visible = not mainFrame.Visible
end)

if not notificationShown then
game.StarterGui:SetCore("SendNotification", {
Title = "Script By Sad_00022",
Text = "ÿµÿßŸÜÿπ ÿßŸÑÿ≥ŸÉÿ±ÿ®ÿ™ Sad_00022",
Duration = 8
})
notificationShown = true
end

